<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="mailer_lite.service.subscribers" class="MailerLiteBundle\Service\SubscribersService">
            <argument>%mailer_lite.config%</argument>
        </service>

        <service id="mailer_lite.service.lists" class="MailerLiteBundle\Service\ListsService">
            <argument>%mailer_lite.config%</argument>
        </service>
    </services>


</container>
